<%- include("../partials/header") %>
<div class="row">
    <div class="col-md-4 ">
        <div class="thumbnail">
            <h1 id="name"><%=user.firstName +" "+user.lastName %></h1>
            <img src="<%=user.avatar%>" alt="user profile image">
            <div class="caption text-white"><a href="mailto:<% user.email %>"><%= user.email %> </a></div>


        </div>
      
        

    </div>
    <div class="col-md-8 text-white">
        <h3 id="title">Posts</h3>
        <ul>
            <% campgrounds.forEach(function(campground){ %>
                <li><a href="/campgrounds/<%=campground.id %>"><%= campground.name %></a></li>
    
           <% }) %>
        </ul>
       

       
    </div>
</div>

</div>
<div class="fx text-center"> Made By team f(x)</div>

</div>

<script
  src="https://code.jquery.com/jquery-3.1.1.min.js"
  integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="
  crossorigin="anonymous"></script>

